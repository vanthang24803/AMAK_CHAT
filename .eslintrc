{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn", 
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "args": "none",
          "ignoreRestSiblings": true
        }
      ]
    }
  }
  